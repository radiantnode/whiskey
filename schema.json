          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "bottle_image": {
          "type": "string"
        },
        "distiliary": {
          "type": "object",
          "properties": {
            "uuid": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "location": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
